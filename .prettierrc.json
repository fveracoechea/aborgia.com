{
  "trailingComma": "all",
  "printWidth": 100,
  "arrowParens": "avoid",
  "singleQuote": true,
  "semi": true,
  "importOrder": ["^react(.*)$", "^next/(.*)$", "<THIRD_PARTY_MODULES>", "^shared/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@trivago/prettier-plugin-sort-imports",
    "@svgr/plugin-prettier"
  ]
}
